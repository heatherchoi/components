<div class="l-page {{ modifier }}">
  {% block header %}
    {{ header }}
  {% endblock %}

  <main id="main" class="l-main" role="main" tabindex="-1">
    {% block breadcrumb %}
      {{ breadcrumb }}
    {% endblock %}

    {% block main %}
      {{ main }}
    {% endblock %}
  </main>

  {% block footer %}
    {{ footer }}
  {% endblock %}

  {% block modal %}
    {{ modal }}
  {% endblock %}
</div>