{% embed '@layouts/page/page.twig' %}
   {% block header %}
    {% include '@layouts/regions/header/header.twig' %}
  {% endblock %}
  {% block breadcrumb %}
    {% include '@layouts/regions/breadcrumb/breadcrumb.twig' %}
  {% endblock %}
  {% block main %}
    {% include '@layouts/regions/highlighted/highlighted.twig' %}
    {% embed '@templates/page.twig' %}
      {% block main %}
        {% include "@components/page-title/page-title.twig" with {
          'page_title': page_title
        } %}
        {% include '@layouts/section/section.twig' with {
          'constrain': '',
          'content': page_content
        } %}
      {% endblock %}
    {% endembed %}
  {% endblock %}
  {% block footer %}
    {% include '@layouts/regions/footer/footer.twig' %}
  {% endblock %}
{% endembed %}
